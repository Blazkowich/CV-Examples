from ultralytics import YOLO
from ultralytics.yolo.v8.detect.predict import DetectionPredictor
import cv2
import numpy as np

model = YOLO('yolov8s.pt')

results = model.predict(source='https://www.youtube.com/watch?v=Y1jTEyb3wiI&ab_channel=RileyK',
                        show=True, conf=0.3, tracker="bytetrack.yaml")

while cv2.waitKey(1) != ord('q'):
    pass
cv2.destroyAllWindows()
